<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<artifactId>SubisTravelContainer</artifactId>
		<groupId>co.com.tauLabs</groupId>
		<version>1.0</version>
	</parent>

	<groupId>co.com.tauLabs</groupId>
	<artifactId>SubisTravelCommonEAR</artifactId>
	<version>1.0</version>
	<packaging>ear</packaging>

	<name>SubisTravelCommonEAR</name>

	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
	</properties>
	<dependencies>
		<dependency>
			<groupId>co.com.tauLabs</groupId>
			<artifactId>SubisTravelWeb</artifactId>
			<version>1.0</version>
			<scope>provided</scope>
			<type>war</type>
		</dependency>
		<dependency>
			<groupId>co.com.tauLabs</groupId>
			<artifactId>TauLabsEntidadesEJB</artifactId>
			<version>1.0</version>
			<scope>provided</scope>
			<type>ejb</type>
		</dependency>
		<dependency>
			<groupId>co.com.tauLabs</groupId>
			<artifactId>TauLabsItemsEJB</artifactId>
			<version>1.0</version>
			<scope>provided</scope>
			<type>ejb</type>
		</dependency>
		<dependency>
			<groupId>co.com.tauLabs</groupId>
			<artifactId>TauLabsUtils</artifactId>
			<version>1.0</version>
			<scope>provided</scope>
			<type>jar</type>
		</dependency>
		<dependency>
			<groupId>co.com.tauLabs</groupId>
			<artifactId>SubisTravelCommon</artifactId>
			<version>1.0</version>
			<scope>provided</scope>
			<type>jar</type>
		</dependency>
		<dependency>
			<groupId>co.com.tauLabs</groupId>
			<artifactId>TauLabsPersistence</artifactId>
			<version>1.0</version>
			<scope>provided</scope>
			<type>ejb</type>
		</dependency>
		<dependency>
			<groupId>com.sun.jersey</groupId>
			<artifactId>jersey-client</artifactId>
			<version>1.19</version>
		</dependency>
		<dependency>
			<groupId>javax.xml</groupId>
			<artifactId>jaxb-api</artifactId>
			<version>2.1</version>
		</dependency>
		<dependency>
			<groupId>com.sun.jersey</groupId>
			<artifactId>jersey-server</artifactId>
			<version>1.19</version>
		</dependency>
		<dependency>
			<groupId>com.sun.jersey</groupId>
			<artifactId>jersey-core</artifactId>
			<version>1.19</version>
		</dependency>
		<dependency>
			<groupId>com.sun.jersey</groupId>
			<artifactId>jersey-servlet</artifactId>
			<version>1.19</version>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.1</version>
				<configuration>
					<source>1.7</source>
					<target>1.7</target>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-ear-plugin</artifactId>
				<version>2.8</version>
				<configuration>
					<version>6</version>
					<defaultLibBundleDir>lib</defaultLibBundleDir>
					<modules>
						<ejbModule>
							<groupId>co.com.tauLabs</groupId>
							<artifactId>TauLabsEntidadesEJB</artifactId>
						</ejbModule>
						<ejbModule>
							<groupId>co.com.tauLabs</groupId>
							<artifactId>TauLabsItemsEJB</artifactId>
						</ejbModule>
						<jarModule>
							<groupId>co.com.tauLabs</groupId>
							<artifactId>TauLabsUtils</artifactId>
						</jarModule>
						<jarModule>
							<groupId>co.com.tauLabs</groupId>
							<artifactId>SubisTravelCommon</artifactId>
						</jarModule>
						<ejbModule>
							<groupId>co.com.tauLabs</groupId>
							<artifactId>TauLabsPersistence</artifactId>
						</ejbModule>
						<webModule>
							<groupId>co.com.tauLabs</groupId>
							<artifactId>SubisTravelWeb</artifactId>
						</webModule>
					</modules>
				</configuration>
			</plugin>
		</plugins>
		<finalName>SubisTravelCommonEAR</finalName>
	</build>
</project>
